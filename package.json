{
  "name": "twitchwatcher",
  "version": "1.0.0",
  "description": "A service that watches twitch webhooks of influencers on Collate",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js 8080",
    "docker-build": "docker build -t collate/twitch-watcher .",
    "docker-run": "npm run docker-build && docker run -p 8000:8080 -v ${pwd}:/app collate/twitch-watcher",
    "webhook-env:start": "ngrok http 8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collatedev/twitch-watcher.git"
  },
  "author": "Evan Coulson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/collatedev/twitch-watcher/issues"
  },
  "homepage": "https://github.com/collatedev/twitch-watcher#readme",
  "dependencies": {
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "nodemon": "^1.18.9"
  }
}
